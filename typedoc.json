{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "src/index.ts"
  ],
  "entryPointStrategy": "Expand",
  "out": "docs",
  "plugin": [
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-djs-links"
  ],
  "theme": "default",
  "tsconfig": "tsconfig.json",
  "name": "Reciple",
  "json": "docs/docs.json",
  "includeVersion": true,
  "excludeExternals": true,
  "hideGenerator": true,
  "githubPages": true
}