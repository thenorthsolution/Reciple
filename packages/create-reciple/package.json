{
  "name": "create-reciple",
  "description": "A Reciple Discord bot project builder",
  "license": "GPL-3.0",
  "version": "9.6.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "bin": "./dist/bin.js",
  "keywords": [
    "reciple",
    "template"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/Reciple",
    "directory": "packages/create-reciple"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "check": "tsc --noEmit",
    "docs": "node ../docgen/dist/bin.js -i ./src/index.ts -o ./docs/docs.json",
    "prepack": "npm run build"
  },
  "files": [
    "dist",
    "templates",
    "assets",
    "README.md"
  ],
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "@reciple/core": "^9.3.2",
    "@reciple/decorators": "^9.0.0",
    "@reciple/utils": "^9.3.1",
    "commander": "^12.1.0",
    "compressing": "^1.10.1",
    "detect-indent": "^7.0.1",
    "discord.js": "^14.15.3",
    "fallout-utility": "^2.9.1",
    "ora": "^8.0.1",
    "package-json": "^10.0.0",
    "reciple": "^9.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "nodemon": "^3.1.4",
    "rimraf": "^5.0.7",
    "typescript": "^5.5.3"
  },
  "gitHead": "88c2b7a5d61ca5707db84435871c11d50259d056"
}
