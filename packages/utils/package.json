{
  "name": "@reciple/utils",
  "description": "Utilities used by reciple modules",
  "license": "GPL-3.0",
  "version": "9.3.2",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "keywords": [
    "reciple",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/thenorthsolution/Reciple",
    "directory": "packages/utils"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./tsconfig.json": "./assets/tsconfig.json"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsup",
    "check": "tsc --noEmit",
    "docs": "node ../docgen/dist/bin.js -i ./src/index.ts -o ./docs/docs.json",
    "prepack": "npm run build"
  },
  "files": [
    "dist",
    "assets",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@discordjs/collection": "^2.1.0",
    "discord-api-types": "^0.37.97",
    "dotenv": "^16.4.5",
    "fallout-utility": "^2.9.1",
    "file-type": "^19.4.1",
    "package-json": "^10.0.1",
    "semver": "^7.6.3"
  },
  "devDependencies": {
    "@types/semver": "^7.5.8",
    "discord.js": "^14.15.3"
  },
  "gitHead": "88c2b7a5d61ca5707db84435871c11d50259d056"
}
